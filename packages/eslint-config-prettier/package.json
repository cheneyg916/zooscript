{
  "name": "@zooscript/eslint-config-prettier",
  "version": "1.0.3",
  "description": "eslint prettier通用规则",
  "keywords": [
    "eslint",
    "prettier"
  ],
  "author": "cheney <cheneyg916@163.com>",
  "homepage": "https://github.com/cheneyg916/zooscript.git",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "doc/",
    "src/",
    "dist/",
    "CHANGELOG.md",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheneyg916/zooscript.git"
  },
  "scripts": {
    "build": "rm -rf ./dist && cp -r ./src ./dist",
    "doc": "github-readme-to-html && rm -rf ./doc && mv ./dist ./doc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "bugs": {
    "url": "https://github.com/cheneyg916/zooscript/issues"
  },
  "dependencies": {
    "github-readme-to-html": "^1.0.7"
  }
}
